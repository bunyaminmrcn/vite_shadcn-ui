{
  "compilerOptions": {
    "lib": ["es2020", "DOM"],
    "moduleResolution": "node",
   
    "paths": {
      "@/*": ["./@/*"],
      "@app/*": ["./app/*"],

    },
    "target": "es2020",
    "checkJs": false,
    "jsx": "react-jsx"
  },
  "include": ["src/**/*.js", "src/**/*.jsx"],
  "exclude": ["node_modules"]
}
